<h1>New Partnership</h1>

<% form_for(@partnership) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :station %><br />
    <%= collection_select(:partnership, :station_id, @radios, :id, :name, :prompt => "") %>
  </p>
    <p>
    <%= f.label :partner %><br />
    <%= collection_select(:partnership, :partner_id, @radios, :id, :name, :prompt => "") %>
  </p>
    <p>
    <%= f.label :fee %><br />
    <%= f.text_field :fee %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', partnerships_path %>