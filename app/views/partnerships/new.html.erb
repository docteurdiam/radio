<h1>New Partnership</h1>

<% semantic_form_for :relationship, :url => partnerships_path, :html => { :enctype => "multipart/form-data"} do |form| %>
  <%= form.input :radio1, :label => "Station", :required => true, :collection =>  @radios %>
  <%= form.input :radio2, :required => true, :label => "Partner",:collection =>  @radios %>
  <%= form.input :fee, :required => true  %>
  <p>
    <%= form.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', partnerships_path %>