<br/>

<table>
  <tr>
    <th>Login</th>
    <th>Created</th>
    <th>Last Login</th>
    <th style="width: 50px"></th>
  </tr>

  <% @users.each do |user| %>
    <tr class="<%= cycle("even", "odd") %>">
      <td><%=h user.login %></td>
      <td><%=h user.created_at %></td>
      <td><%=h user.last_login_at %></td>
      <td><%= link_to 'Delete', user, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<br />

<%= link_to 'New user', new_user_path %>